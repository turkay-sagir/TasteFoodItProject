@model List<TasteFoodIt.Entities.SocialMedia>
<div class="col-md-6 col-lg-3">
    <div class="ftco-footer-widget mb-4">
        <h2 class="ftco-heading-2">BİZİ TAKİP ET</h2>
        <p>Bizi sosyal medya hesaplarımızdan takip edebilir ve güncel bilgilere ulaşabilirsiniz!</p>
        <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-3">
            @foreach (var item in Model)
            {
                <li class="ftco-animate"><a href="@item.RedirectUrl" target="_blank"><span class="@item.IconUrl"></span></a></li>
            }
        </ul>
    </div>
</div>
